generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model MaturedLiveBroilers {
id  String @id @default(auto())  @map("_id") @db.ObjectId
name String
image String
price Int
weight String
rating Int
itemLeft Int
isBestSeller Boolean
dateAdded String
brand String

}
model DayOldBroilers {
id  String @id @default(auto())  @map("_id") @db.ObjectId
name String
image String
price Int
rating Int
itemLeft Int
isBestSeller Boolean
dateAdded String
brand String

}


